<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:googlequery/pi1/locallang_db.xml:pi_flexform.sheetGeneral</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>

                    <targetId>
                        <TCEforms>
                                <label>LLL:EXT:googlequery/pi1/locallang_db.xml:pi_flexform.targetId</label>
                               <config>
                                        <type>group</type>
                                        <internal_type>db</internal_type>
                                        <allowed>pages</allowed>
                                        <size>1</size>
                                        <maxitems>1</maxitems>
                                        <minitems>0</minitems>
                                        <show_thumbs>0</show_thumbs>
                               </config>
                        </TCEforms>
                    </targetId>
                    <templateFile>
                        <TCEforms>
                            <label>LLL:EXT:googlequery/pi1/locallang_db.xml:pi_flexform.TemplateFile</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                                <wizards type="array">
                                    <_PADDING>2</_PADDING>
                                    <link type="array">
                                        <type>popup</type>
                                        <title>Link</title>
                                        <icon>link_popup.gif</icon>
                                        <script>browse_links.php?mode=wizard&amp;act=file</script>
                                        <params type="array">
                                            <blindLinkOptions>page,url,mail,spec</blindLinkOptions>
                                            <allowedExtensions>htm,html,tmpl,tpl</allowedExtensions>
                                        </params>
                                        <JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
                                    </link>
                                </wizards>
                            </config>
                        </TCEforms>
                    </templateFile>
                    <searchEngineType>
                        <TCEforms>
                            <label>LLL:EXT:googlequery/pi1/locallang_db.xml:pi_flexform.searchEngine</label>
                            <onChange>reload</onChange>
	                        <config>
                                <type>select</type>
								<items>
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:googlequery/pi1/locallang_db.xml:pi_flexform.SearchEngineTypeGSA</numIndex>
										<numIndex index="1">gsa</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:googlequery/pi1/locallang_db.xml:pi_flexform.SearchEngineTypeGSS</numIndex>
										<numIndex index="1">gss</numIndex>
									</numIndex>
								</items>
								<minitems>1</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
                        </TCEforms>
                    </searchEngineType>
                    <gsa_host>
                        <TCEforms>
                            <label>LLL:EXT:googlequery/pi1/locallang_db.xml:pi_flexform.gsa_host</label>
                            <config>
                                <type>input</type>
                                <checkbox></checkbox>
                                <size>100</size>
                            </config>
	                        <displayCond>FIELD:searchEngineType:=:gsa</displayCond>
                        </TCEforms>
                    </gsa_host>
                    <frontend>
                        <TCEforms>
                            <label>LLL:EXT:googlequery/pi1/locallang_db.xml:pi_flexform.frontend</label>
                            <config>
                                <type>input</type>
                                <checkbox></checkbox>
                                <size>100</size>
                            </config>
	                        <displayCond>FIELD:searchEngineType:=:gsa</displayCond>
                        </TCEforms>
                    </frontend>
                    <collection>
                        <TCEforms>
                            <label>LLL:EXT:googlequery/pi1/locallang_db.xml:pi_flexform.collection</label>
                            <config>
                                <type>input</type>
                                <checkbox></checkbox>
                                <size>100</size>
                            </config>
	                        <displayCond>FIELD:searchEngineType:=:gsa</displayCond>
                        </TCEforms>
                    </collection>
                    <autoss>
                        <TCEforms>
                            <label>LLL:EXT:googlequery/pi1/locallang_db.xml:pi_flexform.autoss</label>
                            <config>
                                <type>check</type>
                            </config>
	                        <displayCond>FIELD:searchEngineType:=:gsa</displayCond>
                        </TCEforms>
                    </autoss>
                    <autosscss>
                        <TCEforms>
                            <label>LLL:EXT:googlequery/pi1/locallang_db.xml:pi_flexform.autosscss</label>
                            <config>
                                <type>input</type>
                                <checkbox></checkbox>
                                <size>100</size>
                            </config>
	                        <displayCond>FIELD:searchEngineType:=:gsa</displayCond>
                        </TCEforms>
                    </autosscss>
                    <clicklog>
                        <TCEforms>
                            <label>LLL:EXT:googlequery/pi1/locallang_db.xml:pi_flexform.clicklog</label>
                            <config>
                                <type>check</type>
                            </config>
	                        <displayCond>FIELD:searchEngineType:=:gsa</displayCond>
                        </TCEforms>
                    </clicklog>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>